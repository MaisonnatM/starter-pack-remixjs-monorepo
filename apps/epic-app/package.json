{
  "name": "@epic-stack-monorepo/epic-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build --sourcemap",
    "build:server": "tsx ./other/build-server.ts",
    "predev": "pnpm run build:icons --silent",
    "dev": "remix dev -c \"node ./server/dev-server.js\" --manual",
    "prisma:studio": "prisma studio",
    "generate": "prisma generate",
    "db:migrate:force": "prisma migrate reset --force",
    "db:seed": "prisma db seed",
    "format": "prettier --write .",
    "lint": "eslint .",
    "setup": "pnpm run build && prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node .",
    "typecheck": "tsc"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/server-build"
  ],
  "dependencies": {
    "@conform-to/react": "^1.0.2",
    "@conform-to/zod": "^1.0.2",
    "@epic-stack-monorepo/ui": "workspace:*",
    "@epic-web/cachified": "^5.1.2",
    "@epic-web/client-hints": "^1.3.0",
    "@epic-web/invariant": "^1.0.0",
    "@epic-web/remember": "^1.0.2",
    "@epic-web/totp": "^1.1.2",
    "@nasa-gcn/remix-seo": "^2.0.0",
    "@prisma/client": "^5.10.2",
    "@remix-run/express": "^2.7.2",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "@remix-run/router": "^1.15.1",
    "@remix-run/server-runtime": "^2.7.2",
    "address": "^2.0.1",
    "autoprefixer": "^10.4.17",
    "aws-sdk": "^2.1584.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "close-with-grace": "^1.3.0",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "execa": "^8.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "get-port": "^7.0.0",
    "helmet": "^7.1.0",
    "intl-parse-accept-language": "^1.0.0",
    "isbot": "^5.1.0",
    "morgan": "^1.10.0",
    "prisma": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^2.1.0",
    "remix-utils": "^7.5.0",
    "source-map-support": "^0.5.21",
    "spin-delay": "^1.2.0",
    "tailwindcss": "^3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@epic-stack-monorepo/eslint-config": "workspace:*",
    "@remix-run/dev": "^2.7.2",
    "@remix-run/eslint-config": "^2.7.2",
    "@remix-run/serve": "^2.7.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie": "^0.6.0",
    "@types/eslint": "^8.56.3",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/set-cookie-parser": "^2.4.7",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "chokidar": "^3.6.0",
    "cookie": "^0.6.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "jsdom": "^24.0.0",
    "node-html-parser": "^6.1.12",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "remix-flat-routes": "^0.6.4",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "engines": {
    "node": "18"
  },
  "epic-stack": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
